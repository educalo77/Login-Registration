{
  "version": 2,
  "public": false,
  "builds": [
    { "src": "package.json", "use": "@now/static-build" },
    { "src": "/api/server.js", "use": "@now/node-server" }
  ],
  "routes": [
    { "src": "/server/.*", "dest": "/server/index.js" },
    { "src": "^/static/(.*)", "dest": "/static/$1" },
    { "src": ".*", "dest": "/public/index.html" }
  ]
}